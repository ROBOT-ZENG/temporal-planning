		( AT ?OBJ0 ?LOCATION2 )
		( AT ?TRUCK1 ?LOCATION2 )
		( NOT ( ACTIVE-LOAD-TRUCK ?OBJ0 ?TRUCK1 ?LOCATION2 ) )
		( NOT ( STACK2 ) )
		( COUNT0-AT ?OBJ0 ?LOCATION2 )
	)
  :EFFECT
