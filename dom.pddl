		( NOT ( AT ?OBJ0 ?LOCATION2 ) )
		( ACTIVE-LOAD-TRUCK ?OBJ0 ?TRUCK1 ?LOCATION2 )
		( WHEN
			( COUNT0-AT ?TRUCK1 ?LOCATION2 )
			( AND
				( NOT ( COUNT0-AT ?TRUCK1 ?LOCATION2 ) )
				( COUNT1-AT ?TRUCK1 ?LOCATION2 )
			)
		)
		( WHEN
			( COUNT1-AT ?TRUCK1 ?LOCATION2 )
			( AND
				( NOT ( COUNT1-AT ?TRUCK1 ?LOCATION2 ) )
				( COUNT2-AT ?TRUCK1 ?LOCATION2 )
